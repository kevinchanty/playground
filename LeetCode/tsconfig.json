{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "esNext",
            "dom"
        ],
        "sourceMap": true,
        "jsx": "react",
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noUnusedLocals": false,
        "downlevelIteration": true,
    },
    "exclude": [
        "node_modules",
        "build",
        "scripts",
        "index.js"
    ]
}